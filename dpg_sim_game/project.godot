; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Label",
"class": "AutoSizeLabel",
"language": "GDScript",
"path": "res://Tools/auto_size_label.gd"
}, {
"base": "Node2D",
"class": "GameTooltip",
"language": "GDScript",
"path": "res://Scenes/Common/Tooltip.gd"
}, {
"base": "Node2D",
"class": "MainGame",
"language": "GDScript",
"path": "res://Game.gd"
}, {
"base": "Control",
"class": "PointCounter",
"language": "GDScript",
"path": "res://Scenes/MainSession/PointCounter.gd"
}, {
"base": "Sprite",
"class": "PointParticle",
"language": "GDScript",
"path": "res://Scenes/MainSession/PointParticle.gd"
} ]
_global_script_class_icons={
"AutoSizeLabel": "",
"GameTooltip": "",
"MainGame": "",
"PointCounter": "",
"PointParticle": ""
}

[application]

config/name="DPG Sim"
run/main_scene="res://Game.tscn"
config/icon="res://icon.png"

[autoload]

global="*res://global.gd"
trans="*res://trans.gd"

[display]

window/size/width=640
window/size/height=640
window/stretch/mode="2d"
window/stretch/aspect="expand"

[gui]

common/drop_mouse_on_gui_input_disabled=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_clear_color=Color( 0.607843, 0.670588, 0.698039, 1 )
environment/default_environment="res://default_env.tres"
